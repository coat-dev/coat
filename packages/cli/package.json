{
  "name": "@coat/cli",
  "version": "0.0.1",
  "author": "Peter Juras <me@peterjuras.com>",
  "bin": {
    "coat": "./build/bin/cli.js"
  },
  "dependencies": {
    "commander": "^5.1.0",
    "execa": "^4.0.0",
    "fs-extra": "^9.0.0",
    "import-from": "^3.0.0",
    "inquirer": "^7.1.0",
    "semver": "^7.3.2",
    "type-fest": "^0.13.1",
    "validate-npm-package-name": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@types/fs-extra": "^8.1.0",
    "@types/inquirer": "^6.5.0",
    "@types/jest": "^25.2.1",
    "@types/semver": "^7.1.0",
    "@types/tmp": "^0.1.0",
    "@types/validate-npm-package-name": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "inquirer-test": "^2.0.1",
    "jest": "^25.5.1",
    "memfs": "^3.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "tmp": "^0.2.0",
    "typescript": "~3.8.3"
  },
  "engines": {
    "node": ">=10"
  },
  "keywords": [
    "coat"
  ],
  "license": "MIT",
  "scripts": {
    "build": "run-p build:*",
    "build:babel": "babel src -d build --extensions \".ts,.ts\" --ignore \"**/*.d.ts,**/*.test.ts\"",
    "build:typedefs": "tsc -p tsconfig.build.json",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint \"{src,test}/**/*{.ts,.js}\" --max-warnings 0",
    "lint:prettier": "prettier --check src test",
    "lint:tsc": "tsc",
    "prebuild": "rm -rf build",
    "prepack": "npm run build",
    "test": "cross-env JEST_PROJECT=\"Unit tests\" jest",
    "test:integration": "cross-env JEST_PROJECT=\"Integration tests\" jest --coverage=false",
    "test:all": "jest"
  }
}
