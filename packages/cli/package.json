{
  "name": "@coat/cli",
  "version": "0.0.20",
  "author": "Peter Juras <me@peterjuras.com>",
  "bin": {
    "coat": "bin/coat.js"
  },
  "dependencies": {
    "ajv": "7.2.1",
    "boxen": "5.0.0",
    "chalk": "4.1.0",
    "commander": "7.1.0",
    "execa": "5.0.0",
    "fs-extra": "9.1.0",
    "immer": "8.0.1",
    "import-from": "3.0.0",
    "import-local": "3.0.2",
    "inquirer": "7.3.3",
    "js-yaml": "4.0.0",
    "json-colorizer": "2.2.2",
    "json-stable-stringify": "1.0.1",
    "lodash": "4.17.21",
    "ora": "5.3.0",
    "prettier": "2.2.1",
    "resolve-from": "5.0.0",
    "semver": "7.3.4",
    "sha3": "2.1.4",
    "single-trailing-newline": "1.0.0",
    "tmp": "0.2.1",
    "type-fest": "0.21.2",
    "wrap-ansi": "7.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.13.0",
    "@babel/core": "7.13.8",
    "@babel/preset-env": "7.13.9",
    "@babel/preset-typescript": "7.13.0",
    "@types/fs-extra": "9.0.8",
    "@types/inquirer": "7.3.1",
    "@types/jest": "26.0.20",
    "@types/js-yaml": "4.0.0",
    "@types/json-stable-stringify": "1.0.32",
    "@types/lodash": "4.14.168",
    "@types/semver": "7.3.4",
    "@types/single-trailing-newline": "1.0.0",
    "@types/tmp": "0.2.0",
    "@types/wrap-ansi": "3.0.0",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/parser": "4.16.1",
    "concurrently": "6.0.0",
    "cross-env": "7.0.3",
    "eslint": "7.21.0",
    "eslint-config-prettier": "8.1.0",
    "jest": "26.6.3",
    "jest-circus": "26.6.3",
    "lint-staged": "10.5.4",
    "memfs": "3.2.0",
    "memory-streams": "0.1.3",
    "strip-ansi": "6.0.0",
    "ts-node": "9.1.1",
    "typescript": "4.2.3",
    "typescript-json-schema": "0.49.0",
    "which": "2.0.2"
  },
  "engines": {
    "node": ">=10"
  },
  "keywords": [
    "coat"
  ],
  "license": "MIT",
  "main": "build/index.js",
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix --max-warnings=0",
      "prettier --write"
    ]
  },
  "scripts": {
    "build": "concurrently npm:build:*",
    "build:babel": "babel src -d build --extensions \".ts\" --ignore \"**/*.d.ts,**/*.test.ts\"",
    "build:typedefs": "tsc -p tsconfig.build.json",
    "build:generate": "ts-node scripts/create-schemas.ts",
    "lint": "concurrently npm:lint:*",
    "lint:eslint": "eslint \"{src,test}/**/*{.ts,.js}\" --max-warnings 0",
    "lint:prettier": "prettier --check src test",
    "lint:tsc": "tsc",
    "lint-staged": "lint-staged",
    "prebuild": "rm -rf build",
    "prepack": "cross-env BABEL_ENV=production npm run build",
    "test": "cross-env JEST_PROJECT=\"Unit tests\" jest",
    "test:e2e": "cross-env JEST_PROJECT=\"E2E tests\" jest --coverage=false",
    "test:e2e-test-session-create": "ts-node scripts/e2e-test-session-create",
    "test:e2e-test-session-destroy": "ts-node scripts/e2e-test-session-destroy",
    "test:all": "jest",
    "watch-build": "concurrently npm:watch-build:*",
    "watch-build:babel": "npm run build:babel -- --watch",
    "watch-build:typedefs": "npm run build:typedefs -- --watch",
    "watch-lint:tsc": "npm run lint:tsc -- --watch"
  }
}
