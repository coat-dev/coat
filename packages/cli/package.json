{
  "name": "@coat/cli",
  "version": "0.0.15",
  "author": "Peter Juras <me@peterjuras.com>",
  "bin": {
    "coat": "./bin/coat.js"
  },
  "dependencies": {
    "boxen": "4.2.0",
    "chalk": "4.1.0",
    "commander": "6.2.0",
    "execa": "4.1.0",
    "fs-extra": "9.0.1",
    "immer": "8.0.0",
    "import-from": "3.0.0",
    "inquirer": "7.3.3",
    "js-yaml": "3.14.0",
    "json-colorizer": "2.2.2",
    "json-stable-stringify": "1.0.1",
    "lodash": "4.17.20",
    "ora": "5.1.0",
    "prettier": "2.2.0",
    "resolve-from": "5.0.0",
    "semver": "7.3.2",
    "sha3": "2.1.3",
    "single-trailing-newline": "1.0.0",
    "type-fest": "0.20.1",
    "tmp": "0.2.1",
    "wrap-ansi": "7.0.0"
  },
  "bundledDependencies": [
    "boxen",
    "chalk",
    "commander",
    "execa",
    "fs-extra",
    "immer",
    "import-from",
    "inquirer",
    "js-yaml",
    "json-colorizer",
    "json-stable-stringify",
    "lodash",
    "ora",
    "prettier",
    "resolve-from",
    "semver",
    "sha3",
    "single-trailing-newline",
    "tmp",
    "type-fest",
    "wrap-ansi"
  ],
  "devDependencies": {
    "@babel/cli": "7.12.8",
    "@babel/core": "7.12.9",
    "@babel/preset-env": "7.12.7",
    "@babel/preset-typescript": "7.12.7",
    "@types/fs-extra": "9.0.4",
    "@types/inquirer": "7.3.1",
    "@types/jest": "26.0.15",
    "@types/js-yaml": "3.12.5",
    "@types/json-stable-stringify": "1.0.32",
    "@types/lodash": "4.14.165",
    "@types/semver": "7.3.4",
    "@types/single-trailing-newline": "1.0.0",
    "@types/tmp": "0.2.0",
    "@typescript-eslint/eslint-plugin": "4.8.2",
    "@typescript-eslint/parser": "4.8.2",
    "@types/wrap-ansi": "3.0.0",
    "concurrently": "5.3.0",
    "cross-env": "7.0.2",
    "eslint": "7.14.0",
    "eslint-config-prettier": "6.15.0",
    "jest": "26.6.3",
    "jest-circus": "26.6.3",
    "memfs": "3.2.0",
    "memory-streams": "0.1.3",
    "strip-ansi": "6.0.0",
    "typescript": "4.1.2",
    "ts-node": "9.0.0",
    "which": "2.0.2"
  },
  "engines": {
    "node": ">=10"
  },
  "keywords": [
    "coat"
  ],
  "license": "MIT",
  "main": "build/index.js",
  "scripts": {
    "build": "concurrently npm:build:*",
    "build:babel": "babel src -d build --extensions \".ts,.ts\" --ignore \"**/*.d.ts,**/*.test.ts\"",
    "build:typedefs": "tsc -p tsconfig.build.json",
    "lint": "concurrently npm:lint:*",
    "lint:eslint": "eslint \"{src,test}/**/*{.ts,.js}\" --max-warnings 0",
    "lint:prettier": "prettier --check src test",
    "lint:tsc": "tsc",
    "prebuild": "rm -rf build",
    "prepare": "npm run build && ts-node scripts/sync-bundled-dependencies",
    "prepack": "cross-env BABEL_ENV=production npm run build",
    "test": "cross-env JEST_PROJECT=\"Unit tests\" jest",
    "test:e2e": "cross-env JEST_PROJECT=\"E2E tests\" jest --coverage=false",
    "test:e2e-test-session-create": "ts-node scripts/e2e-test-session-create",
    "test:e2e-test-session-destroy": "ts-node scripts/e2e-test-session-destroy",
    "test:all": "jest",
    "watch-build": "concurrently npm:watch-build:*",
    "watch-build:babel": "npm run build:babel -- --watch",
    "watch-build:typedefs": "npm run build:typedefs -- --watch",
    "watch-lint:tsc": "npm run lint:tsc -- --watch"
  }
}
