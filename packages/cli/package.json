{
  "name": "@coat/cli",
  "version": "0.0.2",
  "author": "Peter Juras <me@peterjuras.com>",
  "bin": {
    "coat": "./build/bin/cli.js"
  },
  "dependencies": {
    "commander": "5.1.0",
    "execa": "4.0.3",
    "fs-extra": "9.0.1",
    "import-from": "3.0.0",
    "inquirer": "7.3.3",
    "json-stable-stringify": "1.0.1",
    "lodash": "4.17.20",
    "prettier": "2.1.1",
    "resolve-from": "5.0.0",
    "semver": "7.3.2",
    "single-trailing-newline": "1.0.0",
    "type-fest": "0.16.0",
    "validate-npm-package-name": "3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.10.5",
    "@babel/core": "7.11.4",
    "@babel/node": "7.10.5",
    "@babel/preset-env": "7.11.0",
    "@babel/preset-typescript": "7.10.4",
    "@types/fs-extra": "9.0.1",
    "@types/inquirer": "7.3.1",
    "@types/jest": "26.0.10",
    "@types/json-stable-stringify": "1.0.32",
    "@types/lodash": "4.14.160",
    "@types/semver": "7.3.3",
    "@types/tmp": "0.2.0",
    "@types/validate-npm-package-name": "3.0.0",
    "@typescript-eslint/eslint-plugin": "3.10.1",
    "@typescript-eslint/parser": "3.10.1",
    "cross-env": "7.0.2",
    "eslint": "7.7.0",
    "eslint-config-prettier": "6.11.0",
    "inquirer-test": "2.0.1",
    "jest": "26.4.2",
    "jest-circus": "26.4.2",
    "memfs": "3.2.0",
    "npm-run-all": "4.1.5",
    "tmp": "0.2.1",
    "typescript": "4.0.2"
  },
  "engines": {
    "node": ">=10"
  },
  "keywords": [
    "coat"
  ],
  "license": "MIT",
  "scripts": {
    "build": "run-p build:*",
    "build:babel": "babel src -d build --extensions \".ts,.ts\" --ignore \"**/*.d.ts,**/*.test.ts\"",
    "build:typedefs": "tsc -p tsconfig.build.json",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint \"{src,test}/**/*{.ts,.js}\" --max-warnings 0",
    "lint:prettier": "prettier --check src test",
    "lint:tsc": "tsc",
    "prebuild": "rm -rf build",
    "prepack": "cross-env BABEL_ENV=production npm run build",
    "test": "cross-env JEST_PROJECT=\"Unit tests\" jest",
    "test:integration": "cross-env JEST_PROJECT=\"Integration tests\" jest --coverage=false",
    "test:all": "jest",
    "watch-build": "run-p watch-build:*",
    "watch-build:babel": "npm run build:babel -- --watch",
    "watch-build:typedefs": "npm run build:typedefs -- --watch"
  }
}
