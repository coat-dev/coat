{
  "name": "@coat/template-ts-package",
  "version": "0.0.51",
  "author": "Peter Juras <me@peterjuras.com>",
  "devDependencies": {
    "@babel/cli": "7.23.4",
    "@babel/core": "7.23.6",
    "@babel/preset-env": "7.23.6",
    "@babel/preset-typescript": "7.23.3",
    "@coat/cli": "0.0.28",
    "@types/fs-extra": "11.0.4",
    "@types/jest": "29.5.11",
    "@types/tmp": "0.2.6",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "fs-extra": "11.2.0",
    "jest": "29.7.0",
    "lint-staged": "14.0.1",
    "prettier": "2.8.8",
    "rimraf": "5.0.5",
    "tmp": "0.2.1",
    "typescript": "5.3.3"
  },
  "files": [
    "build/",
    "files/"
  ],
  "keywords": [
    "coat",
    "template",
    "coat-template",
    "typescript"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix --max-warnings=0",
      "prettier --write"
    ]
  },
  "main": "build/index.js",
  "peerDependencies": {
    "@coat/cli": "^0.0.28"
  },
  "scripts": {
    "build": "coat run build:*",
    "build:babel": "babel src -d build --extensions \".ts\" --ignore \"**/*.test.ts\"",
    "build:typedefs": "tsc -p tsconfig.build.json",
    "coat": "coat",
    "lint": "coat run lint:*",
    "lint-staged": "lint-staged",
    "lint:eslint": "eslint --ext .ts --max-warnings 0 src",
    "lint:prettier": "prettier --check src",
    "lint:types": "tsc",
    "prebuild": "rimraf build",
    "prepack": "coat run build",
    "test": "jest",
    "watch-build": "coat run build:* --watch",
    "watch-build:babel": "npm run build:babel -- --watch",
    "watch-build:typedefs": "npm run build:typedefs -- --watch",
    "watch-lint:tsc": "npm run lint:types -- --watch"
  }
}
