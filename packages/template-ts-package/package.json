{
  "name": "@coat/template-ts-package",
  "version": "0.0.10",
  "author": "Peter Juras <me@peterjuras.com>",
  "devDependencies": {
    "@babel/cli": "7.11.6",
    "@babel/core": "7.11.6",
    "@babel/preset-env": "7.11.5",
    "@babel/preset-typescript": "7.10.4",
    "@coat/cli": "0.0.11",
    "@types/fs-extra": "9.0.1",
    "@types/jest": "26.0.14",
    "@types/tmp": "0.2.0",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "babel-jest": "26.3.0",
    "eslint": "7.10.0",
    "eslint-config-prettier": "6.12.0",
    "fs-extra": "9.0.1",
    "jest": "26.4.2",
    "jest-circus": "26.4.2",
    "memfs": "3.2.0",
    "prettier": "2.1.2",
    "rimraf": "3.0.2",
    "tmp": "0.2.1",
    "typescript": "4.0.3"
  },
  "files": [
    "build/",
    "files/"
  ],
  "keywords": [
    "coat",
    "template",
    "coat-template",
    "typescript"
  ],
  "license": "MIT",
  "main": "build/index.js",
  "peerDependencies": {
    "@coat/cli": "^0.0.11"
  },
  "scripts": {
    "build": "coat run build:*",
    "build:babel": "babel src -d build --extensions \".ts,.tsx\" --ignore \"**/*.test.ts,**/*.test.tsx\"",
    "build:typedefs": "tsc -p tsconfig.build.json --declaration --emitDeclarationOnly",
    "lint": "coat run lint:*",
    "lint:eslint": "eslint \"src/**/*.{ts,tsx}\" --max-warnings 0",
    "lint:prettier": "prettier --check src",
    "lint:types": "tsc",
    "prebuild": "rimraf build",
    "prepare": "coat run build && coat sync",
    "test": "jest",
    "watch-build": "coat run npm:watch-build:*",
    "watch-build:babel": "npm run build:babel -- --watch",
    "watch-build:typedefs": "npm run build:typedefs -- --watch",
    "watch-lint:tsc": "npm run lint:types -- --watch"
  }
}
